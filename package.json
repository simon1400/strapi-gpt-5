{
  "version": "0.1.2",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run clean && strapi-plugin build && npm run build:types",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "clean": "rimraf dist",
    "build:types": "tsc -p admin/tsconfig.build.json && tsc -p server/tsconfig.build.json"
  },
  "dependencies": {
    "openai": "^5"
  },
  "devDependencies": {
    "@strapi/strapi": "^5.23.6",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/typescript-utils": "^5.23.6",
    "@strapi/design-system": "2.0.0-rc.29",
    "@strapi/icons": "2.0.0-rc.29",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19",
    "react-intl": "^6.6.2",
    "typescript": "^5.9.2",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5",
    "@strapi/sdk-plugin": "^5",
    "@strapi/design-system": ">=2.0.0-rc.18 <3",
    "@strapi/icons": ">=2.0.0-rc.18 <3",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6",
    "styled-components": "^6",
    "react-intl": ">=6.6.2 <7"
  },
  "strapi": {
    "kind": "plugin",
    "name": "strapi-gpt-5",
    "displayName": "GPT-5 strapi",
    "description": "Plugin Chat GPT-5 for strapi v5"
  },
  "name": "@simon1400/strapi-gpt-5",
  "description": "Plugin Chat GPT-5 for strapi v5",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.18 <=22.x"
  },
  "author": "Dmytro Pechunka <dmytro@pechunka.com>"
}
